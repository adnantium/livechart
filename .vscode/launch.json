  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Agent",
      "type": "debugpy",
      "request": "launch",
      "program": "livechartai/agent.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/chart/"
    },
    {
      "name": "Debug: Django",
      "type": "debugpy",
      "request": "launch",
      "args": ["runserver"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/chart/manage.py"
    },

    {
      "name": "Debug: patient_care",
      "type": "debugpy",
      "request": "launch",
      "program": "livechartai/patient_care.py",
      "cwd": "${workspaceFolder}/chart/",
      "console": "integratedTerminal"
    }
  ]
}
